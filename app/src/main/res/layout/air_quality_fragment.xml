<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".AirQualityFragment">

    <TextView
        android:id="@+id/textView21"
        android:layout_width="257dp"
        android:layout_height="57dp"
        android:lineSpacingExtra="4.5sp"
        android:text="@string/device"
        android:textColor="#ffe5e5"
        android:textSize="33sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.135"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.066" />

    <ImageView
        android:id="@+id/imageline"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="56dp"
        android:src="@drawable/component_line"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView21" />

    <TextView
        android:id="@+id/textView20"
        android:layout_width="266dp"
        android:layout_height="43dp"
        android:lineSpacingExtra="5sp"
        android:text="@string/firedetector"
        android:textColor="#ffe5e5"
        android:textSize="24sp"
        android:textStyle="normal"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.165"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.168" />

    <TextView
        android:id="@+id/textView7"
        android:layout_width="266dp"
        android:layout_height="43dp"
        android:lineSpacingExtra="5sp"
        android:text="@string/gaslevel"
        android:textColor="#ffe5e5"
        android:textSize="24sp"
        android:textStyle="normal"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.165"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.26" />

    <TextView
        android:id="@+id/textView3"
        android:layout_width="266dp"
        android:layout_height="43dp"
        android:lineSpacingExtra="5sp"
        android:text="@string/seems_ok"
        android:textColor="#ffe5e5"
        android:textSize="24sp"
        android:textStyle="normal"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.165"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.308" />

    <TextView
        android:id="@+id/mq2_co_data"
        android:layout_width="360dp"
        android:layout_height="39dp"
        android:text="default data"
        android:textColor="#ffe5e5"
        android:textSize="24sp"
        android:textStyle="normal"
        app:layout_constraintBottom_toTopOf="@+id/mq2_result"
        app:layout_constraintEnd_toEndOf="@+id/mq2_result"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="@+id/mq2_result"
        app:layout_constraintTop_toBottomOf="@+id/mq2_lpg_data" />

    <TextView
        android:id="@+id/mq2_lpg_data"
        android:layout_width="358dp"
        android:layout_height="35dp"
        android:text="default data"
        android:textColor="#ffe5e5"
        android:textSize="24sp"
        android:textStyle="normal"
        app:layout_constraintBottom_toTopOf="@+id/mq2_co_data"
        app:layout_constraintEnd_toEndOf="@+id/mq2_co_data"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="@+id/mq2_co_data"
        app:layout_constraintTop_toBottomOf="@+id/mq2_smoke_data" />

    <TextView
        android:id="@+id/mq2_smoke_data"
        android:layout_width="359dp"
        android:layout_height="30dp"
        android:text="default data"
        android:textColor="#ffe5e5"
        android:textSize="24sp"
        android:textStyle="normal"
        app:layout_constraintBottom_toTopOf="@+id/mq2_lpg_data"
        app:layout_constraintEnd_toEndOf="@+id/mq2_lpg_data"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="@+id/mq2_lpg_data"
        app:layout_constraintTop_toBottomOf="@+id/textView3" />

    <ImageView
        android:layout_width="53dp"
        android:layout_height="53dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.514"
        app:layout_constraintStart_toEndOf="@+id/textView3"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.283"
        app:srcCompat="@drawable/air_quality_thumbsup" />

    <TextView
        android:id="@+id/mq2_result"
        android:layout_width="225dp"
        android:layout_height="45dp"
        android:layout_marginBottom="4dp"
        android:text="Within Ideal Range"
        android:textColor="#FFFFFF"
        android:textSize="24sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/mq2_co_data" />

</androidx.constraintlayout.widget.ConstraintLayout>